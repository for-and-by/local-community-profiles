{
  "name": "@safer-streets/db",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate-db-types": "supabase gen types typescript --project-id \"ostfogwbvvrspkozoelv\" --schema public > src/generated-types.ts"
  },
  "dependencies": {
    "@supabase/auth-helpers-remix": "^0.1.9",
    "@supabase/supabase-js": "^2.12.0",
    "base64-arraybuffer": "^1.0.2",
    "maplibre-gl": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "prettier": "^2.8.3",
    "supabase": "^1.44.0",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
