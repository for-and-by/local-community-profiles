    "react-dom": "^18.2.0",
    "cross-env": "^7.0.3",
    "maplibre-gl": "^2.4.0",
    "@react-spring/web": "^9.5.5",
    "@supabase/supabase-js": "^1.35.6",
    "base64-arraybuffer": "^1.0.2",
    "clsx": "^1.2.1",
    "immer": "^9.0.15",
    "marked": "^4.0.18",
    "react-hook-form": "^7.36.1",
    "scheduler": "^0.23.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.12.0",
